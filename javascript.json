{
	"Boilerplate": {
	  "prefix": "html5",
	  "body": [
		"<!DOCTYPE html>",
		"<html lang='en'>",
		"<head>",
		"  <meta charset='UTF-8'>",
		"  <title></title>",
		"</head>",
		"<body>",
		"  $0",
		"</body>",
		"</html>"
	  ],
	  "description": "Generates HTML5 Boilerplate"
	},
  
	"Bullets": {
	  "prefix": "bullets",
	  "body": ["<ul>", "  <li>$1</li>", "</ul>"],
	  "description": "Adds bullet point list to HTML file"
	},
  
	"Print to console": {
	  "prefix": "log",
	  "body": ["console.log('$1');", "$2"],
	  "description": "Log output to console"
	}
  }